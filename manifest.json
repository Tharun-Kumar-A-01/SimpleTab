{
  "manifest_version": 3,
  "name": "SimpleTab",
  "version": "0.2",
  "description": "A simple Homepage",
  "author": "Tharun Kumar",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "host_permissions": [
    "https://icons.duckduckgo.com/*",
    "https://*/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "SimpleTab@TharunKumar.com"
    }
  }
}
